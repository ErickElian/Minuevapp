<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Tarea2</title>
    <link rel="stylesheet" href="~/css/sb-admin-2.css" />
    <link rel="stylesheet" href="~/css/sb-admin-2.min" />
    <link rel="stylesheet" href="~/scss/sb-admin-2.scss" />
    <link rel="stylesheet" href="~/scss/_buttons.scss" />
    <link rel="stylesheet" href="~/scss/_cards.scss" />
    <link rel="stylesheet" href="~/scss/_charts.scss" />
    <link rel="stylesheet" href="~/scss/_dropdowns.scss" />
    <link rel="stylesheet" href="~/scss/_error.scss" />
    <link rel="stylesheet" href="~/scss/_footer.scss" />
    <link rel="stylesheet" href="~/scss/_global.scss" />
    <link rel="stylesheet" href="~/scss/_login.scss" />
    <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://cdn-icons-png.flaticon.com/128/346/346167.png"
          rel="stylesheet">
</head>
<body>

    <div id="wrapper">
        <partial name="_Sidebar.cshtml" /> 
        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                <partial name="_Topbar.cshtml" />
                <div class="container-fluid">
                    <main role="main" class="pb-3">
                        @RenderBody()
                    </main>
                </div>
            </div>
        </div>
    </div>

    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <partial name="_Footer.cshtml" />
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/js/sb-admin-2.min.js"></script> 
    <script src="https://kit.fontawesome.com/a1fa0d85ae.js" crossorigin="anonymous"></script>

    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>